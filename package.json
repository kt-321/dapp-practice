{
  "name": "dapp-starter",
  "version": "0.0.1",
  "scripts": {
    "build": "npx hardhat compile --network localhost && next build",
    "compile": "yarn hardhat compile && yarn hardhat typechain",
    "deploy:ERC20KV1:local": "npx hardhat run scripts/deploy_ERC20KV1.ts --network localhost",
    "deploy:ERC721K:local": "npx hardhat run scripts/deploy_ERC721K.ts --network localhost",
    "deploy:ERC721KV1:goerli": "npx hardhat run scripts/deploy_ERC721KV1.ts --network goerli",
    "deploy:ERC721KV1:local": "npx hardhat run scripts/deploy_ERC721KV1.ts --network localhost",
    "deploy:ERC721KV2:local": "npx hardhat run scripts/deploy_ERC721KV2.ts --network localhost",
    "dev": "next dev",
    "hardhat": "npx hardhat node",
    "lint": "next lint",
    "lint:sol": "npx solhint 'contracts/**/*.sol'",
    "merkle": "npx node merkle_tree.mjs",
    "prepare": "husky install",
    "start": "next start",
    "test": "npx hardhat test"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.5.1",
    "@ethersproject/providers": "5.7.2",
    "@ethersproject/units": "5.7.0",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.4",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts-upgradeable": "^4.8.0",
    "@openzeppelin/hardhat-upgrades": "^1.22.0",
    "@tailwindcss/typography": "0.5.8",
    "@typechain/hardhat": "^6.1.5",
    "@web3-react/core": "6.1.9",
    "@web3-react/injected-connector": "6.0.7",
    "@web3-react/network-connector": "6.2.9",
    "@web3-react/walletconnect-connector": "6.2.13",
    "chai": "^4.3.7",
    "daisyui": "2.46.0",
    "dotenv": "^16.0.3",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.4",
    "hardhat-gas-reporter": "^1.0.9",
    "keccak256": "^1.0.6",
    "loglevel": "1.8.1",
    "merkletreejs": "^0.3.9",
    "next": "13.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "solidity-coverage": "^0.8.2",
    "web3-react": "5.0.5"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "4.8.0",
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@typechain/ethers-v5": "10.2.0",
    "@types/chai": "4.3.4",
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.47.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.30.0",
    "eslint-config-next": "13.0.7",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.2",
    "nano-staged": "0.8.0",
    "postcss": "8.4.20",
    "prettier": "2.8.1",
    "prettier-plugin-solidity": "1.1.0",
    "rustywind": "0.15.1",
    "solgraph": "1.0.2",
    "solhint": "3.3.7",
    "sort-package-json": "2.1.0",
    "tailwindcss": "3.2.4",
    "ts-node": "10.9.1",
    "typechain": "8.1.1",
    "typescript": "4.9.4"
  },
  "nano-staged": {
    "*.{js,ts,tsx}": [
      "eslint --cache --quiet --fix",
      "prettier --write",
      "rustywind --write"
    ],
    "*.{css,json,md,yml,yaml}": [
      "prettier --write"
    ],
    "package.json": "sort-package-json"
  }
}
